<launch>
  <node pkg="lrf_scan" type="lrf_scan_node" name="lrf_scan" output="screen">
    <param name="scan_pub_name" value="front_scan" />
    <param name="frame_id" value="front_scan" />
	<param name="eth_flg" value="true" />
    <param name="ip_address" value="192.168.10.21" />
    <param name="ip_port" value="10940" />
    <param name="serial_port" value="/dev/ttyACM0" />
    <param name="serial_baud" value="115200" />
    <param name="publish_intensity" value="true" />
    <param name="degree_minimum" value="-113.5" />
    <param name="degree_maximum" value="113.5" />
    <param name="skip" value="0" />
    <param name="range_minimum" value="0.5" />
    <param name="range_maximum" value="1.5" />
  </node>
</launch>
