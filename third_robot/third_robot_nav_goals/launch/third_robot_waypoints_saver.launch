<launch>

  <!-- Run the map server -->
  <node name="map_server" 
  		pkg="map_server" 
		type="map_server" args="$(find third_robot_gmapping)/map/20151006_ctrl_building.yaml" />
  
  <node pkg="third_robot_nav_goals" 
		type="third_robot_waypoints_saver" 
		name="third_robot_waypoints_saver"
		output="screen">
    <param name="savewaypointsfile" value="$(find third_robot_nav_goals)/waypoints/ctrl_building.csv " />
  </node>

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find third_robot_nav_goals)/config/third_robot_waypoints_saver.rviz" required="true" />

</launch>
